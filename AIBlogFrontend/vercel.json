{
    "routes": [
      {
        "src": "/(.*).html",
        "dest": "/index.html"
      },
      {
        "src": "/(.*).(js|css)",
        "headers": { "cache-control": "public,max-age=31536000,immutable" },
        "continue": true
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  